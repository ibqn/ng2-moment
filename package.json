{
  "name": "ng2-moment",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server -c bs-config.js\" ",
    "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup.config.js && node copy-dist-files.js",
    "lite:aot": "lite-server -c aot/bs-config.js",
    "lite": "lite-server -c bs-config.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
